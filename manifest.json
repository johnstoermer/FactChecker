{
  "manifest_version": 2,
  "name": "FactChecker",
  "description": "Check Your Facts",
  "version": "1.0",
  "browser_action": {
    "default_title": "Fact Checker",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["app.js"],
      "run_at": "document_end"
    } 
  ]
}
